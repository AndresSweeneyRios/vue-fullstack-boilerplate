{
  "name": "vue-fullstack",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "express:start": "node server/index.js",
    "express:dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "cross-env NODE_ENV=development vue-cli-service build",
    "dev": "multiscript yarn vue:serve + yarn express:dev",
    "start": "yarn run vue:build && yarn run express:start",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "chalk": "^3.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-router": "^3.1.5",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^9.5.0",
    "multiscript": "^0.1.8",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
